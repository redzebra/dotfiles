have ec2-cmd && {
	_aws_apitools_ec2() {
		local cur prev opts
		COMPREPLY=()
		cur=${COMP_WORDS[COMP_CWORD]}
		prev=${COMP_WORDS[COMP_CWORD-1]}

		case ${prev} in
			--cert|--private-key)
				_filedir
				return 0
				;;
			--user-data-file|-f)
				_filedir
				return 0
				;;
			--instance-initiated-shutdown-behaviour)
				COMPREPLY=( $(compgen -W "stop terminate" -- ${cur}) )
				return 0
				;;
			--instance-type|-t)
				COMPREPLY=( $(compgen -W "m1.small m1.medium m1.large m1.xlarge m3.xlarge m3.2xlarge t1.micro m2.xlarge m2.2xlarge m2.4xlarge c1.medium c1.xlarge cc1.4xlarge cc2.8xlarge cg1.4xlarge hi1.4xlarge" -- ${cur}) )
				return 0
				;;
			--region)
				COMPREPLY=( $(compgen -W "ap-northeast-1 ap-southeast-1 ap-southeast-2 eu-west-1 sa-east-1 us-east-1 us-west-1 us-west-2" -- ${cur}) )
				return 0
				;;
		esac

		if [[ ${cur} == -* ]]; then
			for opt in $(${COMP_WORDS[0]} -h|egrep --color=never '^ +-([A-Za-z]|(-[a-z]+)+)'); do
				[[ ${opt} = -* ]] && opts+="${opts:+ }${opt%,}"
			done
			COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
			return 0
		fi
	}

  complete -F _aws_apitools_ec2 ec2-activate-license
  complete -F _aws_apitools_ec2 ec2-add-group
  complete -F _aws_apitools_ec2 ec2-add-keypair
