		'--auto-scaling-group[Auto Scaling Group to be deleted]:name'
		'(-d --force-delete)'{-d,--force-delete}'[asynchronously delete the group]'
		'(-f --force)'{-f,--force}'[delete without prompting]'
	)
	_arguments : $arguments
}

