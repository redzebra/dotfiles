_cfn-list-stacks () {
	typeset -A opt_args
	local context state line

	arguments=(
		$_aws_apitools_cfn_general_options
		'(-s --stack-status)'{-s,--stack-status}'[Status(es) of the stacks to return]:stack_status:_values -s , CREATE_IN_PROGRESS CREATE_FAILED CREATE_COMPLETE ROLLBACK_IN_PROGRESS ROLLBACK_FAILED ROLLBACK_COMPLETE DELETE_IN_PROGRESS DELETE_FAILED DELETE_COMPLETE UPDATE_IN_PROGRESS UPDATE_COMPLETE_CLEANUP_IN_PROGRESS UPDATE_COMPLETE UPDATE_ROLLBACK_IN_PROGRESS UPDATE_ROLLBACK_FAILED UPDATE_ROLLBACK_COMPLETE_CLEANUP_IN_PROGRESS UPDATE_ROLLBACK_COMPLETE'
	)
	_arguments $arguments
}

_cfn-update-stack () {
	typeset -A opt_args
	local context state line

	arguments=(
		$_aws_apitools_cfn_general_options
		'--stack-name[Name or Stack Id of the stack]:stack_name'
		'(-c --capabilities)'{-c,--capabilities}'[explicitly approved capabilities that may be used]:capabilities:__aws_apitools_cfn_capabilities'
   	'(-d --disable-rollback)'{-d,--disable-rollback}'[disable rollback of created resources on failure]'
   	'(-f -u --template-file --template-url)'{-f,--template-file}'[file that contains the template]:template_file:_files'
   	'(-p --parameters)'{-p,--parameters}'[Parameter values used to create the stack]:parameters'
   	'(-f -u --template-file --template-url)'{-u,--template-url}'[URL that contains the template]:template_url'
	)
	_arguments $arguments
}

_cfn-validate-template () {
	typeset -A opt_args
	local context state line

	arguments=(
		$_aws_apitools_cfn_general_options
		'(-f -u --template-file --template-url)'{-f,--template-file}'[file that contains the template]:file:_files'
		'(-f -u --template-file --template-url)'{-u,--template-url}'[URL that contains the template]:url'
	)
	_arguments $arguments
}

