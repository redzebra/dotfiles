_aws_apitools_cfn_general_options=(
  '(- 1 *)'{'-?',-h,--help}'[Display help]' \
	'(-C -I -K -S --access-key-id --ec2-cert-file-path --ec2-private-key-file-path --secret-key)--aws-credential-file[Location of the file with your AWS credentials]:file:_files'
	'(-I -S --access-key-id --aws-credential-file --secret-key)'{-C,--ec2-cert-file-path}'[file]:file:_files'
	'--connection-timeout[Connection timeout (in seconds)]:number'
	'--delimiter[use delimiter]:delimiter'
	{-H,--headers}'[show headers]'
	'(-C -K --ec2-cert-file-path --ec2-private-key-file-path)'{-I,--access-key-id}'[access key id]:id'
	'(-I -S --access-key-id --secret-key)'{-K,--ec2-private-key-file-path}'[private key file path]:file:_files'
	'--region[Amazon EC2 region to use]:region:__aws_apitools_cfn_region'
	'(-C -K --aws-credential-file --ec2-private-key-file-path)'{-S,--secret-key}'[The AWS Secret Key to use]:file:_files'
	'--show-empty-fields[Shows empty fields and rows]'
	'--show-request[show request]'
	'(--show-long --show-xml --quiet)--show-table[Display tabular output]'
	'(--show-table --show-xml --quiet)--show-long[Display delimited (long) output]'
	'(--show-long --show-table --quiet)--show-xml[Display XML output]'
	'(--show-long --show-table --show-xml)--quiet[quiet]'
	'(--region)'{-U,--url}'[URL]:url'
)

__aws_apitools_cfn_region () {
  _values 'region' \
    'ap-northeast-1[Asia Pacific (Tokyo) Region]' \
    'ap-southeast-1[Asia Pacific (Singapore) Region]' \
    'ap-southeast-2[Asia Pacific (Sydney) Region]' \
    'eu-west-1[EU (Ireland) Region]' \
    'sa-east-1[South America (Sao Paulo) Region]' \
    'us-east-1[US East (Northern Virginia) Region]' \
    'us-west-1[US West (Northern California) Region]' \
    'us-west-2[US West (Oregon) Region]'
}

_cfn-cancel-update-stack () {
	typeset -A opt_args
	local context state line

	_arguments $_aws_apitools_cfn_general_options
}

