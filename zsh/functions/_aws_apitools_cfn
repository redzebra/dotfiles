_cfn-describe-stack-resource () {
	typeset -A opt_args
	local context state line

	arguments=(
		$_aws_apitools_cfn_general_options
		'--stack-name[Name or Stack Id of the stack]:stack_name'
		'--logical-resource-id[Logical identifier of the resource]:logical_resource_id'
	)
	_arguments $arguments
}

_cfn-describe-stack-resources () {
	typeset -A opt_args
	local context state line

	arguments=(
		$_aws_apitools_cfn_general_options
		'(-l --logical-resource-id)'{-l,--logical-resource-id}'[Logical identifier of the resource]:logical_resource_id'
		'(-p --physical-resource-id)'{-p,--physical-resource-id}'[Physical identifier of the resource]:physical_resource_id'
		'(-s --stack-name)'{-s,--stack-name}'[Name or Stack Id of the stack]:stack_name'
	)
	_arguments $arguments
}

